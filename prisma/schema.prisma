// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Budget {
  id             Int @id @default(autoincrement())
  income         Int
  savings        Int
  housing        Int
  food           Int
  education      Int
  recreation     Int
  clothing       Int
  communications Int
  personalCare   Int
  insurance      Int
  transportation Int
  medical        Int
  fees           Int
}
