// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Budget {
  id             Int @id @default(autoincrement())
  income         Int @default(0)
  savings        Int @default(0)
  housing        Int @default(0)
  food           Int @default(0)
  education      Int @default(0)
  recreation     Int @default(0)
  clothing       Int @default(0)
  communications Int @default(0)
  personalCare   Int @default(0)
  insurance      Int @default(0)
  transportation Int @default(0)
  medical        Int @default(0)
  fees           Int @default(0)
}
